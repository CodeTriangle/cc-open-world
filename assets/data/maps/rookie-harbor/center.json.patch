                "name": "SchneiderExplainsQuests", "eventType": "CUTSCENE", "endCondition": "plot.autumnIntro >= 20", "event": [ { "pos": { "x": 960, "y": 488 }, "speed": "NORMAL", "transition": "EASE_IN_OUT", "wait": false, "waitSkip": 0, "zoom": 1, "type": "SET_CAMERA_POS" }, { "entity": { "player": true }, "action": [ { "value": 0.5, "type": "SET_RELATIVE_SPEED" }, { "target": { "x": 932, "y": 532, "lvl": "1" }, "maxTime": 0, "distance": 0, "precise": false, "planOnly": false, "type": "NAVIGATE_TO_POINT" } ], "repeating": false, "wait": false, "keepState": false, "immediately": false, "type": "DO_ACTION" }, { "entity": { "global": true, "name": "Emilie" }, "action": [ { "value": 0.5, "type": "SET_RELATIVE_SPEED" }, { "target": { "x": 988, "y": 532, "lvl": "1" }, "maxTime": 0, "distance": 0, "precise": false, "planOnly": false, "type": "NAVIGATE_TO_POINT" }, { "face": "NORTH", "rotate": true, "rotateSpeed": 3, "type": "SET_FACE" } ], "repeating": false, "wait": false, "keepState": false, "immediately": false, "type": "DO_ACTION" }, { "entity": { "global": true, "name": "Schneider" }, "action": [ { "value": 0.5, "type": "SET_RELATIVE_SPEED" }, { "target": { "x": 960, "y": 488, "lvl": "1" }, "maxTime": 0, "distance": 0, "precise": false, "planOnly": false, "type": "NAVIGATE_TO_POINT" }, { "face": "SOUTH", "rotate": true, "rotateSpeed": 3, "type": "SET_FACE" } ], "repeating": false, "wait": false, "keepState": false, "immediately": false, "type": "DO_ACTION" }, { "entity": { "player": true }, "type": "WAIT_UNTIL_ACTION_DONE" }, { "entity": { "player": true }, "action": [ { "entity": { "global": true, "name": "Schneider" }, "rotate": true, "rotateSpeed": 3, "type": "SET_FACE_TO_ENTITY" } ], "repeating": true, "wait": false, "keepState": false, "immediately": false, "type": "DO_ACTION" }, { "entity": { "global": true, "name": "Schneider" }, "type": "WAIT_UNTIL_ACTION_DONE" }, { "entity": { "global": true, "name": "Emilie" }, "type": "WAIT_UNTIL_ACTION_DONE" }, { "entity": { "player": true }, "action": [ { "face": "NORTH", "rotate": true, "rotateSpeed": 3, "type": "SET_FACE" } ], "repeating": false, "wait": false, "keepState": false, "immediately": false, "type": "DO_ACTION" }, { "side": "LEFT", "order": 0, "clearSide": false, "type": "ADD_MSG_PERSON", "person": { "person": "main.schneider", "expression": "DEFAULT" } }, { "side": "RIGHT", "order": 4, "clearSide": false, "type": "ADD_MSG_PERSON", "person": { "person": "main.emilie", "expression": "DEFAULT" } }, { "side": "RIGHT", "order": 0, "clearSide": false, "type": "ADD_MSG_PERSON", "person": { "person": "main.lea", "expression": "DEFAULT" } }, { "message": { "en_US": "See that building up there?", "de_DE": "Seht ihr das Gebäude dort oben?", "fr_FR": "fr_FR", "langUid": 231, "zh_CN": "看见那边那栋大楼了吗？", "ja_JP": "あそこにある建物が見えるか？", "ko_KR": "저쪽에 있는 건물 보여?", "zh_TW": "看見那邊那棟大樓了嗎？" }, "autoContinue": false, "type": "SHOW_MSG", "person": { "person": "main.schneider", "expression": "DEFAULT" } }, { "side": "ALL", "type": "CLEAR_MSG" }, { "pos": { "x": 960, "y": 184 }, "speed": "NORMAL", "transition": "EASE_IN_OUT", "wait": true, "waitSkip": 0.2, "zoom": 1, "type": "SET_CAMERA_POS" }, { "side": "LEFT", "order": 0, "clearSide": false, "type": "ADD_MSG_PERSON", "person": { "person": "main.schneider", "expression": "DEFAULT" } }, { "message": { "en_US": "That's the \\c[3]Seeker Hub\\c[0] or \\c[3]Quest Hub\\c[0].", "de_DE": "Das ist der \\c[3]Seeker Hub\\c[0] oder \\c[3]Quest Hub\\c[0].", "fr_FR": "fr_FR", "langUid": 232, "zh_CN": "那就是\\c[3]探索者中心\\c[0]，或者说\\c[3]任务中心\\c[0]。", "ja_JP": "あそこが\\c[3]探索者ハブ\\c[0]、または\\c[3]クエストハブ\\c[0]だ。", "ko_KR": "저곳이 바로 \\c[3]시커 허브\\c[0] 또는 \\c[3]퀘스트 허브\\c[0]야.", "zh_TW": "那就是\\c[3]探索者中心\\c[0]，或者說\\c[3]任務中心\\c[0]。" }, "autoContinue": false, "type": "SHOW_MSG", "person": { "person": "main.schneider", "expression": "UP" } }, { "message": { "en_US": "Surprise, you'll find a lot of \\c[3]quests\\c[0] there!", "de_DE": "Dort findet ihr - wer hätt's gedacht? - eine Meng