        "content": [
					{ "entity": { "party": "Member2" }, "skipEffects": false, "type": "HIDE_ENTITY" }, { "entity": { "party": "Member3" }, "skipEffects": false, "type": "HIDE_ENTITY" }, { "alpha": 1, "time": 0, "lighter": true, "type": "SET_OVERLAY", "color": "white" }, { "pos": { "x": 400, "y": 728 }, "speed": "IMMEDIATELY", "transition": "EASE", "wait": false, "waitSkip": 0, "zoom": 1, "type": "SET_CAMERA_POS" }, { "ignoreSlowDown": false, "type": "WAIT", "time": 0.2 }, { "zoomType": "LIGHT", "fadeIn": 0, "duration": 2, "fadeOut": 0.5, "type": "SET_ZOOM_BLUR" }, { "alpha": 0, "time": 2, "lighter": true, "type": "SET_OVERLAY", "color": "white" }, { "pos": { "x": 400, "y": 380 }, "speed": "SLOWER", "transition": "EASE_OUT", "wait": true, "waitSkip": 0, "zoom": 1, "type": "SET_CAMERA_POS" }, { "text": { "en_US": "Your journey brought you to this\nforest of the eternal night.", "de_DE": "Deine Reise f\u00fchrte dich \nin den Wald der ewigen Nacht.", "zh_CN": "\u65c5\u9014\u5e26\u4f60\u6765\u5230\u8fd9\u4e2a\n\u6c38\u591c\u4e4b\u6797\u3002<<A<<[CHANGED 2018/08/28]", "ja_JP": "\u6c5d\u306f\u3053\u306e\u6c38\u9060\u306e\u591c\u304c\u7d9a\u304f\u68ee\u307e\u3067\n\u65c5\u8def\u3092\u305f\u3069\u3063\u3066\u304d\u305f\u3002<<A<<[CHANGED 2018/08/28]", "ko_KR": "\ub2f9\uc2e0\uc740 \uc5ec\uc815\uc744 \ud1b5\ud574\n\uc774 \uc601\uc6d0\ud55c \ubc24\uc758 \uc232\uae4c\uc9c0 \uc654\uc2b5\ub2c8\ub2e4.<<A<<[CHANGED 2018/08/28]", "langUid": 2, "zh_TW": "\u65c5\u9014\u5e36\u4f60\u4f86\u5230\u9019\u500b\r\n\u6c38\u591c\u4e4b\u6797\u3002<<A<<[CHANGED 2018/08/28]" }, "entity": { "global": true, "name": "hologramNPC" }, "posType": "TOP", "offset": { "x": 0, "y": 0 }, "type": "SHOW_DREAM_MSG" }, { "text": { "en_US": "Taking on the trials given \nby the creatures of these lands.", "de_DE": "In diesen seinen Landen hast du dich\nvon seinen Gesch\u00f6pfen pr\u00fcfen lassen.", "zh_CN": "\u627f\u8f7d\u6b64\u5730\u751f\u7075\u7684\u613f\u671b\n\u8e0f\u4e0a\u6311\u6218\u4e4b\u8def\u3002<<A<<[CHANGED 2018/09/14]", "ja_JP": "\u3053\u306e\u5730\u306e\u751f\u304d\u7269\u305f\u3061\u304c\n\u4e0e\u3048\u3057\u8a66\u7df4\u306b\u6311\u3093\u3060\u3002<<A<<[CHANGED 2018/09/14]", "ko_KR": "\uc774 \ub545\uc758 \uc0dd\ubb3c\ub4e4\uc774 \ubd80\uc5ec\ud558\ub294\n\uc2dc\ud5d8\uc744 \uc218\ud589\ud558\uba74\uc11c \ub9d0\uc785\ub2c8\ub2e4.<<A<<[CHANGED 2018/09/14]", "langUid": 25, "zh_TW": "\u627f\u8f09\u6b64\u5730\u751f\u9748\u7684\u9858\u671b\r\n\u8e0f\u4e0a\u6311\u6230\u4e4b\u8def\u3002<<A<<[CHANGED 2018/09/14]" }, "entity": { "global": true, "name": "hologramNPC" }, "posType": "TOP", "offset": { "x": 0, "y": 0 }, "type": "SHOW_DREAM_MSG" }, { "text": { "en_US": "You proved your vigor, your patience,\nyour agility, your insight.", "de_DE": "Du hast Kraft, Geduld, \nGewandtheit und Einsicht bewiesen.", "zh_CN": "\u4f60\u8bc1\u660e\u4e86\u81ea\u5df1\u7684\n\u529b\u91cf\uff0c\u8010\u5fc3\uff0c\u7075\u5de7\u4e0e\u667a\u6167\u3002<<A<<[CHANGED 2018/08/28]", "ja_JP": "\u6c5d\u306f\u81ea\u3089\u306e\u6d3b\u529b\u3001\u5fcd\u8010\u529b\u3001\n\u654f\u6377\u3055\u3001\u6d1e\u5bdf\u529b\u3092\u8a3c\u660e\u3057\u305f\u3002<<A<<[CHANGED 2018/08/28]", "ko_KR": "\ub2f9\uc2e0\uc740 \ub2f9\uc2e0\uc758 \ud798, \ub2f9\uc2e0\uc758 \uc778\ub0b4,\n\ub2f9\uc2e0\uc758 \ubbfc\ucca9\uc131, \ub2f9\uc2e0\uc758 \ud1b5\ucc30\ub825\uc744 \uc99d\uba85\ud588\uc2b5\ub2c8\ub2e4.<<A<<[CHANGED 2018/08/28]", "langUid": 26, "zh_TW": "\u4f60\u8b49\u660e\u4e86\u81ea\u5df1\u7684\r\n\u529b\u91cf\uff0c\u8010\u5fc3\uff0c\u9748\u5de7\u8207\u667a\u6167\u3002<<A<<[CHANGED 2018/08/28]" }, "entity": { "global": true, "name": "hologramNPC" }, "posType": "TOP", "offset": { "x": 0, "y": 0 }, "type": "SHOW_DREAM_MSG" }, { "text": { "en_US": "All the virtues to be held\nby a true envoy of change.", "de_DE": "Jene Tugenden, die nur der \nwahre Gesandte des Wandels in sich tr\u00e4gt.", "zh_CN": "\n\u6b64\u4e43\u53d8\u9769\u4e4b\u4f7f\u7684\u8bf8\u591a\u7f8e\u5fb7\u3002<<A<<[CHANGED 2018/09/19]", "ja_JP": "\u5909\u5316\u3092\u53f8\u308b\u771f\u306e\u4f7f\u8005\u304c\u6301\u3064\u3001\n4\u3064\u306e\u7f8e\u5fb3\u3060\u3002<<A<<[CHANGED 2018/09/19]", "ko_KR": "\uc774\uac83\ub4e4\uc740 \uc9c4\uc815\ud55c \ubcc0\ud654\uc758 \uc0ac\uc808\ub2e8\uc774\n\uac00\uc838\uc57c \ud560 \ub355\ubaa9\uc785\ub2c8\ub2e4.<<A<<[CHANGED 2018/09/19]", "langUid": 27, "zh_TW": "_x000D_\n\u6b64\u4e43\u8b8a\u9769\u4e4b\u4f7f\u7684\u8af8\u591a\u7f8e\u5fb7\u3002<<A<<[CHANGED 2018/09/19]" }, "entity": { "global": true, "name": "hologramNPC" }, "posType": "TOP", "offset": { "x": 0, "y": 0 }, "type": "SHOW_DREAM_MSG" }, { "text": { "en_US": "Your accomplishments shall\nbe acknowledged.", "de_DE": "Deine Errungenschaften \nverdienen Anerkennung.", "zh_CN": "\u4f60\u7684\u6210\u5c31\n\u5c06\u88ab\u94ed\u8bb0\u3002<<A<<[CHANGED 2018/08/28]", "ja_JP": "\u6c5d\u304c\u6210\u3057\u9042\u3052\u3057\u3053\u3068\u306f\n\u8a8d\u3081\u3089\u308c\u308b\u3067\u3042\u308d\u3046\u3002<<A<<[CHANGED 2018/08/28]", "ko_KR": "\ub2f9\uc2e0\uc758 \uc5c5\uc801\uc740\n\uc778\uc815\ubc1b\uc744 \ub9cc\ud569\ub2c8\ub2e4.<<A<<[CHANGED 2018/08/28]", "langUid": 28, "zh_TW": "\u4f60\u7684\u6210\u5c31\r\n\u5c07\u88ab\u9298\u8a18\u3002<<A<<[CHANGED 2018/08/28]" }, "entity": { "global": true, "name": "hologramNPC" }, "posType": "TOP", "offset": { "x": 0, "y": 0 }, "type": "SHOW_DREAM_MSG" }, { "text": { "en_US": "Take this Shade to open \nthe path through the northern caves.", "de_DE": "Nimm diese Form und \n\u00f6ffne den Weg zu den n\u00f6rdlichen H\u00f6hlen.", "zh_CN": "\u63a5\u7eb3\u6b64\u8bc1\u5370\n\u6253\u5f00\u901a\u5f80\u5317\u65b9\u6d1e\u7a74\u4e4b\u8def\u3002<<A<<[CHANGED 2018/08/28]", "ja_JP": "\u3053\u306e\u30b7\u30a7\u30fc\u30c9\u3092\u624b\u306b\u53d6\u308a\u3001\n\u5317\u306e\u6d1e\u7a9f\u3078\u306e\u9053\u3092\u958b\u304f\u304c\u3088\u3044\u3002<<A<<[CHANGED 2018/08/28]", "ko_KR": "\ubd81\ucabd \ub3d9\uad74\uc758 \uae38\uc744 \uc5f4\uc5b4\uc904\n\uc774 \uc170\uc774\ub4dc\ub97c \uac00\uc838\uac00\uc2ed\uc2dc\uc624.<<A<<[CHANGED 2018/08/28]", "lan