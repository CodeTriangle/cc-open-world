                    { "frequency": "", "type": "SET_NPC_RUNNERS" }, { "ignoreSlowDown": false, "type": "WAIT", "time": 0.3 }, { "withElse": true, "type": "IF", "condition": "!party.has.Emilie", "thenStep": [ { "type": "START_PRIVATE_MSG" }, { "side": "RIGHT", "order": 0, "clearSide": false, "type": "ADD_MSG_PERSON", "person": { "person": "main.lea", "expression": "DEFAULT" } }, { "side": "LEFT", "order": 0, "clearSide": false, "type": "ADD_MSG_PERSON", "person": { "person": "main.emilie", "expression": "DEFAULT" } }, { "message": { "en_US": "Bonjour, Lea!", "de_DE": "Bonjour, Lea!", "fr_FR": "fr_FR", "langUid": 121, "zh_CN": "\u4f60\u597d\uff0c\u8389\u4e9a\uff01", "ja_JP": "\u30dc\u30f3\u30b8\u30e5\u30fc\u30eb\u3001\u30ec\u30a2\uff01", "ko_KR": "\uc548\ub155, \ub808\uc544!", "zh_TW": "\u4f60\u597d\uff0c\u8389\u4e9e\uff01" }, "autoContinue": false, "type": "SHOW_MSG", "person": { "person": "main.emilie", "expression": "DEFAULT" } }, { "message": { "en_US": "Hi!", "de_DE": "Hi!", "fr_FR": "fr_FR", "langUid": 122, "zh_CN": "\u4f60\u597d\uff01", "ja_JP": "\u3084\u3041\uff01", "ko_KR": "\uc548\ub155!", "zh_TW": "\u4f60\u597d\uff01" }, "autoContinue": false, "type": "SHOW_MSG", "person": { "person": "main.lea", "expression": "EXCITED" } }, { "message": { "en_US": "Guess what?\\. Lukas told me he'd like to introduce us to his guild.", "de_DE": "Wei\u00dft du was?\\. Lukas hat mir gesagt, dass er uns gerne seiner Gilde vorstellen w\u00fcrde.", "fr_FR": "fr_FR", "langUid": 123, "zh_CN": "\u731c\u731c\u600e\u4e48\u7740\uff1f\\.\u5362\u5361\u65af\u544a\u8bc9\u6211\u4ed6\u60f3\u8981\u628a\u6211\u4eec\u4ecb\u7ecd\u7ed9\u4ed6\u7684\u516c\u4f1a\u3002", "ja_JP": "\u306d\u3048\u805e\u3044\u3066\u3088\uff01\\.\u30eb\u30fc\u30ab\u30b9\u304c\u306d\u3001\u79c1\u305f\u3061\u3092\u3042\u3044\u3064\u306e\u30ae\u30eb\u30c9\u306b\u62db\u5f85\u3057\u305f\u3044\u3063\u3066\u3002", "ko_KR": "\uadf8\uac70 \uc54c\uc544?\\. \ub8e8\uce74\uc2a4\uac00 \uc6b0\ub9ac\ub97c \uae38\ub4dc\uc5d0 \uc18c\uac1